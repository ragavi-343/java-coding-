import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s1=sc.next();
        String res="";
        for(int i=0;i<s1.length();i++)
        {
           if(res.indexOf(s1.charAt(i))==-1)
           {
               res+=s1.charAt(i);
           }
        }
        System.out.print(res);
    }
}
