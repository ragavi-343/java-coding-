import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	String s=sc.next();
	String res="";
	for(int i=0;i<s.length();i++)
	{
	    if(res.indexOf(s.charAt(i))==-1)
	    {
	        res+=s.charAt(i);
	    }
	}
	char[] arr=res.toCharArray();
	Arrays.sort(arr);
	String result=new String(arr);
	System.out.print(result);
	}
}
