import java.util.*;
class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       String s=sc.next();
       char[] arr=s.toCharArray();
       int l=0,r=arr.length-1;
       while(l<r)
       {
           char t=arr[l];
           arr[l]=arr[r];
           arr[r]=t;
           l++;
           r--;
       }
       System.out.print(new String(arr));
    }
}
