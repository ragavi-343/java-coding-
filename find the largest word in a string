import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        String[] word=s.split(" ");
        String maxlength="";
        for(int i=0;i<word.length;i++)
        {
            if(word[i].length()>maxlength.length())
            {
                maxlength=word[i];
            }
        }
        System.out.print(maxlength);
    }
}
