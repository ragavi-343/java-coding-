import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        int n=4;
        for(int r=1;r<=n;r++)
        {
            for(int s=n-1;s>r-1;s--)
            {
                System.out.print(" ");
            }
            for(int c=1;c<=2*r-1;c++)
            {
                System.out.print("*");
            }
            System.out.print("\n");
        }
    }
}
